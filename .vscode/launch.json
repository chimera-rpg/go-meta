  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug editor",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/src/go-editor/",
      "cwd": "${workspaceFolder}",
      "env": {},
    },
    {
      "name": "Launch editor",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/bin/editor.exe",
      "cwd": "${workspaceFolder}/bin",
      "env": {},
      "args": ["-wd", "${workspaceFolder}/bin"]
    }

  ]
}